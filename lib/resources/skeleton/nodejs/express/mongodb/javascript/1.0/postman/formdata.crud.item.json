                    "raw": "",
                    "options": {
                        "raw": {
                            "language": "json"
                        }
                    }
                },
                "url": {
                    "raw": "{{local}}/{{{ crudNameCase.kebabCase }}}/:_id",
                    "host": [
                        "{{local}}"
                    ],
                    "path": [
                        "{{{ crudNameCase.kebabCase }}}",
                        ":_id"
                    ],
                    "variable": [
                        {
                            "key": "_id",
                            "value": "111111111111111111111111",
                            "description": "toggle isActive flag for record with _id"
                        }
                    ]
                }
            },
            "response": []
        }
    ]
}