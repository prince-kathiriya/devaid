{
    "name": "{{{ crudNameCase.kebabCase }}}",
    "item": [
        {
            "name": "create-{{{ crudNameCase.kebabCase }}}",
            "request": {
                "method": "POST",
                "header": [
                    {
                        "key": "x-auth-token",
                        "value": "{{x-auth-token}}",
                        "type": "text"
                    },
                    {
                        "key": "local-upload",
                        "value": "true",
                        "description": "will upload files in local if set to true",
                        "type": "text",
                        "disabled": true
                    }
                ],
                "body": {
                    "mode": "formdata",
                    "formdata": {{{ formdata }}}
                },
                "url": {
                    "raw": "{{local}}/{{{ crudNameCase.kebabCase }}}",
                    "host": [
                        "{{local}}"
                    ],
                    "path": [
                        "{{{ crudNameCase.kebabCase }}}"
                    ]
                }
            },
            "response": []
        },
        {
            "name": "fetch-{{{ crudNameCase.kebabCase }}}",
            "protocolProfileBehavior": {
                "disableBodyPruning": true
            },
            "request": {
                "method": "GET",
                "header": [
                    {
                        "key": "x-auth-token",
                        "value": "{{x-auth-token}}",
                        "type": "text"
                    }
                ],
                "body": {
                    "mode": "raw",
                    "raw": "",
                    "options": {
                        "raw": {
                            "language": "json"
                        }
                    }
                },
                "url": {
                    "raw": "{{local}}/{{{ crudNameCase.kebabCase }}}",
                    "host": [
                        "{{local}}"
                    ],
                    "path": [
                        "{{{ crudNameCase.kebabCase }}}"
                    ],
                    "query": [
                        {
                            "key": "_id",
                            "value": "111111111111111111111111",
                            "disabled": true
                        },
                        {
                            "key": "isAll",
                            "value": "true",
                            "description": "returns all records, ignores of isActive flag",
                            "disabled": true
                        },
                        {
                            "key": "isActive",
                            "value": "true",
                            "disabled": true
                        },
                        {
                            "key": "search",
                            "value": "devaid",
                            "disabled": true
                        },
                        {
                            "key": "page",
                            "value": "1",
                            "description": "default: 1",
                            "disabled": true
                        },
                        {
                            "key": "limit",
                            "value": "1000",
                            "description": "default: 100",
                            "disabled": true
                        },
                        {
                            "key": "sortBy",
                            "value": "createdAt",
                            "description": "default: createdAt",
                            "disabled": true
                        },
                        {
                            "key": "sortOrder",
                            "value": "DESC",
                            "description": "default: DESC",
                            "disabled": true
                        }
                    ]
                }
            },
            "response": []
        },
        {
            "name": "update-{{{ crudNameCase.kebabCase }}}",
            "request": {
                "method": "PUT",
                "header": [
                    {
                        "key": "x-auth-token",
                        "value": "{{x-auth-token}}",
                        "type": "text"
                    },
                    {
                        "key": "local-upload",
                        "value": "true",
                        "description": "will upload files in local if set to true",
                        "type": "text",
                        "disabled": true
                    }
                ],
                "body": {
                    "mode": "formdata",
                    "formdata": {{{ formdata }}}
                },
                "url": {
                    "raw": "{{local}}/{{{ crudNameCase.kebabCase }}}/:_id",
                    "host": [
                        "{{local}}"
                    ],
                    "path": [
                        "{{{ crudNameCase.kebabCase }}}",
                        ":_id"
                    ],
                    "variable": [
                        {
                            "key": "_id",
                            "value": "111111111111111111111111",
                            "description": "updates body for record with _id "
                        }
                    ]
                }
            },
            "response": []
        },<% if(multiImagesNames.length > 0) { %>{{{'\n'}}}{
            "name": "pull-images-{{{ crudNameCase.kebabCase }}}",
            "request": {
                "method": "PUT",
                "header": [
                    {
                        "key": "x-auth-token",
                        "value": "{{x-auth-token}}",
                        "type": "text"
                    },
                    {
                        "key": "base64-upload",
                        "value": "true",
                        "type": "text",
                        "disabled": true
                    }
                ],
                "body": {
                    "mode": "raw",
                    "raw": "{\r\n    // _id from multiple images array goes here\r\n    \"_ids\": [\r\n        \"111111111111111111111111\",\r\n        \"111111111111111111111111\"\r\n    ]\r\n}",
                    "options": {
                        "raw": {
                            "language": "json"
                        }
                    }
                },
                "url": {
                    "raw": "{{local}}/{{{ crudNameCase.kebabCase }}}/pull-images",
                    "host": [
                        "{{local}}"
                    ],
                    "path": [
                        "{{{ crudNameCase.kebabCase }}}",
                        "pull-images"
                    ]
                }
            },
            "response": []
        },<% } %>
